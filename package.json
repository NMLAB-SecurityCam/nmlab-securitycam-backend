        "@line/bot-sdk": "^7.5.0",
        "core-js": "^3.22.5",
        "cors": "^2.8.5",
        "dotenv-defaults": "^5.0.0",
        "express": "^4.18.1",
        "grpc": "^1.24.11",
        "mongoose": "^6.3.3",
        "nodemon": "^2.0.16",
        "regenerator-runtime": "^0.13.9"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/node": "^7.17.10",
        "@babel/preset-env": "^7.17.10"
    },
    "prettier": {
        "arrowParens": "avoid",
        "trailingComma": "es5",
        "singleQuote": true,
        "semi": true,
        "tabWidth": 2,
        "useTabs": false,
        "printWidth": 200
